Package: wordnet
Title: WordNet Interface
Version: 0.1-17
Authors@R: c(person("Ingo", "Feinerer", role = "aut"),
             person("Kurt", "Hornik", role = c("aut", "cre"),
                    email = "Kurt.Hornik@R-project.org",
		    comment = c(ORCID = "0000-0003-4198-9911")),
